@model IEnumerable<string>

@Html.ActionLink("Домой","List","Parts",null,new { @class = "btn btn-blok btn-default btn-lg btn btn-blok btn-default btn-lg" })

@foreach (var link in Model)
{
    @Html.RouteLink(link, new
{
    controller = "Parts",
    action = "List",
    cat = link,
    page = 1
}, new
{
    @class = "btn btn - blok btn - default btn- lg btn btn-blok btn-default btn-lg"
    + (link == ViewBag.SelectedCategory ? "btn-primary" : 
    "")
})
}